<div class="item<%= " active" if group == @product_slide_groups.first %>">
  <% group.each do |product| %>
  <div class="col-xs-4 prod">
    <%= link_to (image_tag(product.image_url)), product_path(product) %>
    <div class="carousel-caption">
      <h5><%= product.name %></h5>
    </div>
  </div>
  <% end %>
</div>